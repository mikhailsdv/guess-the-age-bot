{
	"extends": ["eslint:recommended"],
	"rules": {
		"import/no-anonymous-default-export": "off",
		"no-irregular-whitespace": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-extra-semi": "off",
		"prefer-const": "error"
	},
	"env": {"es6": true, "node": true},
	"parserOptions": {
		"ecmaVersion": 2020
	}
}
